<%= form_with(model: [@user,@blog], url: user_blogs_path) do |form| %>
  <div class="field">
    <%= form.label :title%>
    <%= form.text_field :title, autofocus: true %>
  </div>

  <div class="field">
    <%= form.label :description %>
    <%= form.text_field :description %>
  </div>

  <div class="actions">
    <%= form.submit 'New Blog' %>
  </div>
<% end %>